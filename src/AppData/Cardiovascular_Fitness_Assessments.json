    "instructions": [
      {
        "Step": "1",
        "Instruction": "Equipment needed: 12-inch step or bench, metronome or timer, stopwatch"
      },
      {
        "Step": "2",
        "Instruction": "Set metronome to 96 beats per minute (24 steps per minute = 4 counts per step)"
      },
      {
        "Step": "3",
        "Instruction": "Warm up with 2-3 minutes of light stepping or walking"
      },
      {
        "Step": "4",
        "Instruction": "Step up and down continuously for exactly 3 minutes following this pattern: right foot up, left foot up, right foot down, left foot down"
      },
      {
        "Step": "5",
        "Instruction": "Maintain steady rhythm throughout the entire 3 minutes"
      },
      {
        "Step": "6",
        "Instruction": "Immediately after finishing, sit down and rest for exactly 1 minute"
      },
      {
        "Step": "7",
        "Instruction": "At the end of the 1-minute rest, take your pulse for 1 full minute (60 seconds)"
      },
      {
        "Step": "8",
        "Instruction": "Record your 1-minute recovery heart rate and compare to standards"
      }
    ],
    "formula": [
      {
        "Component": "Recovery Heart Rate",
        "Details": "Count pulse for 60 seconds starting exactly 1 minute after finishing the test"
      },
      {
        "Component": "Fitness Score Formula",
        "Details": "Fitness Score = (Duration in seconds × 100) / (Recovery HR × 5.6)"
      },
      {
        "Component": "Duration",
        "Details": "Usually 180 seconds (3 minutes) if completed"
      },
      {
        "Component": "Interpreting Score",
        "Details": "Higher scores indicate better cardiovascular fitness"
      },
      {
        "Component": "Alternative Scoring",
        "Details": "Lower recovery heart rate = better fitness level"
      },
      {
        "Component": "Step Height",
        "Details": "Standard is 12 inches; some protocols use different heights"
      }
    ],
    "notes": [
      {
        "Note": "Equipment",
        "Details": "Use a sturdy 12-inch high step or bench. Some gyms have adjustable aerobic steps."
      },
      {
        "Note": "Metronome",
        "Details": "Free metronome apps available for smartphones to maintain proper cadence"
      },
      {
        "Note": "Step Pattern",
        "Details": "Each complete cycle = 4 beats (up-up-down-down). 96 beats per minute = 24 complete steps per minute"
      },
      {
        "Note": "Safety",
        "Details": "Ensure step is stable and non-slip. Use handrail if needed for balance but not for support"
      },
      {
        "Note": "When to Stop",
        "Details": "Stop if unable to maintain pace for 15 seconds or experience dizziness, pain, or extreme fatigue"
      },
      {
        "Note": "Advantages",
        "Details": "Low impact, requires minimal space, suitable for various fitness levels, good for indoor testing"
      }
    ]
  },
