    "instructions": [
      {
        "Step": "1",
        "Instruction": "Find a measured 1-mile track or course"
      },
      {
        "Step": "2",
        "Instruction": "Warm up with 5-10 minutes of light walking"
      },
      {
        "Step": "3",
        "Instruction": "Walk 1 mile as fast as possible (do not run)"
      },
      {
        "Step": "4",
        "Instruction": "Record your finishing time in minutes and seconds"
      },
      {
        "Step": "5",
        "Instruction": "Immediately after finishing, count your heart rate for 15 seconds and multiply by 4"
      },
      {
        "Step": "6",
        "Instruction": "Record your body weight in pounds"
      },
      {
        "Step": "7",
        "Instruction": "Use the formula to calculate VO2 max or compare time to standards"
      }
    ],
    "formula": [
      {
        "Component": "VO2 Max Formula",
        "Details": "VO2 max = 132.853 - (0.0769 × Weight) - (0.3877 × Age) + (6.315 × Gender) - (3.2649 × Time) - (0.1565 × Heart Rate)"
      },
      {
        "Component": "Gender Value",
        "Details": "Male = 1, Female = 0"
      },
      {
        "Component": "Weight",
        "Details": "Body weight in pounds"
      },
      {
        "Component": "Time",
        "Details": "Walk time in minutes (convert seconds to decimal)"
      },
      {
        "Component": "Heart Rate",
        "Details": "Heart rate in beats per minute immediately after finishing"
      },
      {
        "Component": "Age",
        "Details": "Age in years"
      }
    ]
  },
