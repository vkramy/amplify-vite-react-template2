  "generalGuidelines": [
    {
      "Topic": "When to Test",
      "Details": "Perform tests when well-rested, properly hydrated, and free from illness or injury"
    },
    {
      "Topic": "Test Frequency",
      "Details": "Retest every 8-12 weeks to track cardiovascular fitness improvements"
    },
    {
      "Topic": "Environmental Conditions",
      "Details": "Test in similar weather conditions (temperature, humidity, wind) for accurate comparisons"
    },
    {
      "Topic": "Medical Clearance",
      "Details": "Consult physician before testing if over 40, sedentary, or have health concerns"
    },
    {
      "Topic": "Proper Footwear",
      "Details": "Wear appropriate running shoes with good support and cushioning"
    },
    {
      "Topic": "Hydration",
      "Details": "Drink water before testing but avoid excessive fluid intake immediately before"
    },
    {
      "Topic": "Safety First",
      "Details": "Stop immediately if experiencing chest pain, dizziness, or extreme shortness of breath"
    }
  ]
